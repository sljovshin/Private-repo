{
    "manifest_version"      : 2,
    "name"          : "SL HomePage",
    "description"   : "Dynamic homepage",
    "version"       : "1.0",
    "icons"         : { "128" : "icon_128.png"},
    "browser_action": {
        "default_icon" : "icon.png",
        "default_popup": "popup.html"
    },
    "permissions"   : ["activeTab"],
    "chrome_settings_overrides" : {
        "homepage": "index.html"
      }    
}